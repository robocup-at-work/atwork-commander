<?xml version="1.0"?>

<launch>
  <arg name="refbox" default="atwork_commander"/>
  <arg name="verbose" default="false"/>

  <arg name="file"/>
    <node name="load_helper" pkg="atwork_commander" type="control" args="-v $(arg verbose) load $(arg file)" output="screen">
      <param name="refbox" value="$(arg refbox)"/>
    </node>
</launch>
